{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "dingo/api": "1.0.*@dev",
        "tymon/jwt-auth": "1.0.0-beta.3",
        "aws/aws-sdk-php": "~3.0",
        "league/fractal": "~0.13.0",
        "tucker-eric/eloquentfilter": "^1.2.0",
        "intervention/image": "^2.3",
        "league/flysystem-aws-s3-v3": "~1.0",
        "ramsey/uuid": "^3.4",
        "cartalyst/stripe-laravel": "6.0.*",
        "rtconner/laravel-tagging": "~2.1",
        "league/glide": "^1.0",
        "spatie/laravel-permission": "^2.4.2",
        "barryvdh/laravel-cors": "^0.8.1",
        "graham-campbell/markdown": "^v7.1.0",
        "maatwebsite/excel": "~2.1.0",
        "bugsnag/bugsnag-laravel": "^2.4",
        "pda/pheanstalk": "~3.0",
        "doctrine/dbal": "^2.5",
        "laravel/tinker": "~1.0",
        "laravel/passport": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "laracademy/interactive-make": "^1.1",
        "laravel/browser-kit-testing": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Utilities/v1/Helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/DatabaseSetup.php",
            "tests/BrowserKitTestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
