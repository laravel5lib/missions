{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.*",
        "dingo/api": "1.0.*@dev",
        "aws/aws-sdk-php": "~3.0",
        "league/fractal": "~0.13.0",
        "tucker-eric/eloquentfilter": "^1.2.0",
        "intervention/image": "^2.3",
        "league/flysystem-aws-s3-v3": "~1.0",
        "ramsey/uuid": "^3.4",
        "cartalyst/stripe-laravel": "8.0.*",
        "rtconner/laravel-tagging": "~2.1",
        "league/glide": "^1.0",
        "spatie/laravel-permission": "^2.4.2",
        "barryvdh/laravel-cors": "^0.11.0",
        "maatwebsite/excel": "~2.1.0",
        "bugsnag/bugsnag-laravel": "^2.4",
        "pda/pheanstalk": "~3.0",
        "doctrine/dbal": "^2.5",
        "jeffochoa/factory-stories": "^1.0",
        "laracademy/interactive-make": "^1.1",
        "laravel/tinker": "~1.0",
        "laravel/passport": "^5.0",
        "laracasts/utilities": "^3.0",
        "artesaos/seotools": "^0.10.0",
        "fideloper/proxy": "~4.0",
        "graham-campbell/markdown": "^10.0",
        "spatie/laravel-medialibrary": "^7.0.0",
        "predis/predis": "^1.1",
        "laravel/horizon": "^1.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "filp/whoops": "~2.0",
        "laravel/browser-kit-testing": "^3.0",
        "nunomaduro/collision": "~1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Utilities/v1/Helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
